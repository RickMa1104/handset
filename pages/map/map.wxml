<view >
  <map   id="map"
    longitude="{{longitude}}"
    latitude="{{latitude}}"
    scale="14"
    controls="{{controls}}"
    bindcontroltap="controltap"
    markers="{{markers}}"
    bindmarkertap="markertap"
    polyline="{{polyline}}"
    bindregionchange="regionchange"
    show-location
    style="width: 100%; height: 500px;"
  >
        <button 
        class="scanBt"
        type="primary"
        bindtap="onBack"
      >
      返回个人中心
      </button>

    <view >

    </view>
  </map>
</view>